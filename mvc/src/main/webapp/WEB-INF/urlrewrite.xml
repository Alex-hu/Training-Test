<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 3.0//EN" "http://tuckey.org/res/dtds/urlrewrite3.0.dtd">
<urlrewrite default-match-type="wildcard">
	<rule>
		<from>/*.png</from>
		<to>/*.png</to>
	</rule>
	<rule>
		<from>/*/empty.png</from>
		<to>/*/empty.png</to>
	</rule>
	<rule>
		<from>/code.jpg</from>
		<to>/code.jpg</to>
	</rule>
	<rule>
		<from>/*.jpg</from>
		<to>/*.jpg</to>
	</rule>
	<rule>
		<from>/*.gif</from>
		<to>/*.gif</to>
	</rule>
	<rule>
		<from>**/favicon.ico</from>
		<to>**/favicon.ico</to>
	</rule>
	<rule>
		<from>/login.jsp</from>
		<to>/login.jsp</to>
	</rule>
	<rule>
		<from>/success.jsp</from>
		<to>/success.jsp</to>
	</rule>
	<rule>
		<from>/system/**</from>
		<to>/system/$1</to>
	</rule>
	<rule>
		<from>/common/**</from>
		<to>/common/$1</to>
	</rule>
	<rule>
		<from>/attached/**</from>
		<to>/attached/$1</to>
	</rule>
	<rule>
		<from>/images/**</from>
		<to>/images/$1</to>
	</rule>
	<rule>
		<from>/scripts/**</from>
		<to>/scripts/$1</to>
	</rule>
	<rule>
		<from>/styles/**</from>
		<to>/styles/$1</to>
	</rule>
	<rule>
		<from>/**</from>
		<to>/app/$1</to>
	</rule>
	<outbound-rule>
		<from>/app/**</from>
		<to>/$1</to>
	</outbound-rule>
</urlrewrite>
